<?xml version="1.0" ?>
<movements>
    <movement id="1">
        <!-- draw a square -->
        <set speed="20" x="800" y="500"/>
        <wait t="6"/>
        <moveto x="760" y="500"/>
        <moveto x="760" y="260"/>
        <moveto x="600" y="260"/>
        <moveto x="600" y="450"/>
        
        <wait t="2"/>
        
		<moveto x="600" y="260"/>
      	<moveto x="760" y="260"/>
      	
      	<wait t="3"/>
      	
      	<moveto x="760" y="500"/>

    </movement>

    <movement id="2">
        <set speed="20" x="800" y="500"/>
        <wait t="6"/>
        <moveto x="840" y="500"/>
        <moveto x="840" y="260"/>
        <moveto x="1000" y="260"/>
        <moveto x="1000" y="450"/>
        
        <wait t="2"/>
        
        <moveto x="1000" y="260"/>
        <moveto x="840" y="260"/>
        
        <wait t="3"/>
        
        <moveto x="840" y="500"/>
    </movement>
</movements>